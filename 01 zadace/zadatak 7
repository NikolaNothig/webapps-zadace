var brojevi = [1,2,3,4,3,66,21,4];

//funkcija (metoda) za okretanje redosljeda u polju stringova vec postoji pozivamo ju sa .reverse

console.log(brojevi.reverse());

//ako bas trbamo raditi svoju funkciju, to mozemo napraviti ovako
function obrni(){
    console.log(brojevi.reverse());
}

obrni(brojevi);